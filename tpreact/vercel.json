    {
    "version": 2,
    "builds": [
        { "src": "build/**", "use": "@vercel/static" }
    ],
    "routes": [
        {
        "src": "/(.*).(js|css|png|jpg|jpeg|gif|ico|json)",
        "dest": "/build/$1.$2",
        "headers": {
            "cache-control": "public, max-age=31536000, immutable"
        }
        },
        { "src": "/(.*)", "dest": "/index.html" }
    ]
    }